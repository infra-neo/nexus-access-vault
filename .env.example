# Supabase Configuration
VITE_SUPABASE_PROJECT_ID=your_project_id_here
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
VITE_SUPABASE_URL=https://your-project.supabase.co

# Zitadel Configuration
VITE_ZITADEL_DOMAIN=your-zitadel-domain.com
VITE_ZITADEL_CLIENT_ID=your_client_id
VITE_ZITADEL_PROJECT_ID=your_project_id

# Tailscale Configuration
TAILSCALE_API_KEY=your_tailscale_api_key_here
TAILSCALE_TAILNET=your_tailnet_name

# Pomerium Configuration
POMERIUM_SHARED_SECRET=generate_random_secret_here
POMERIUM_COOKIE_SECRET=generate_random_secret_here

# Email Service (e.g., SendGrid, AWS SES)
EMAIL_SERVICE_API_KEY=your_email_service_key
EMAIL_FROM=noreply@yourdomain.com

# Application Configuration
VITE_APP_URL=https://yourdomain.com
VITE_API_URL=https://api.yourdomain.com

# GCP Configuration (optional)
GCP_PROJECT_ID=your_gcp_project_id
GCP_SERVICE_ACCOUNT_KEY=path_to_service_account_key.json

# LXD/LXC Configuration (optional)
LXD_ENDPOINT=https://your-lxd-server:8443
LXD_CLIENT_CERT=path_to_client_cert.crt
LXD_CLIENT_KEY=path_to_client_key.key
